= form_for @tag_q_relation do |f|
  -if @tag_q_relation.errors.any?
    #error_explanation
      %h2= "#{pluralize(@tag_q_relation.errors.count, "error")} prohibited this tag_q_relation from being saved:"
      %ul
        - @tag_q_relation.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :tag_id
    = f.number_field :tag_id
  .field
    = f.label :question_id
    = f.number_field :question_id
  .actions
    = f.submit 'Save'
